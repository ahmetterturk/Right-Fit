<h1><%= @program.title %></h1>

<p>Description: <%= @program.description %></p>
<p>Duration: <%= @program.duration %></p>
<p>Content: <%= @program.content %></p>
<p>Coach: <%= @program.coach.first_name %></p>
<p>Category: <%= @program.category.category_name %></p>


<% if @program.image.attached? %>
    <%= image_tag @program.image, class: 'card-img-top' %>
<% else %>
    <%= image_tag "blank-program.jpg", class: 'card-img-top' %>
<% end %>
